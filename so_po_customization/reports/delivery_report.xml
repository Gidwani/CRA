                                        <th name="th_sm_sr" class="text-center"><strong>SR #</strong></th>
                                        <th name="th_sb_product" class="text-center"><strong>Products</strong></th>
                                        <th name="th_sb_quantity" class="text-center"><strong>Quantity</strong></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <t t-foreach="backorders" t-as="backorder">
                                        <t t-set="bo_lines" t-value="backorder.move_lines.filtered(lambda x: x.product_uom_qty)"/>
                                        <tr t-foreach="bo_lines" t-as="bo_line" style="border:1px solid black;">
                                            <td class="text-center">
                                                <span t-field="bo_line.number"/>
                                            </td>
                                            <td>
                                                <span t-field="bo_line.product_id"/>
                                                <p t-if="bo_line.description_picking != bo_line.product_id.name">
                                                    <span t-field="bo_line.description_picking"/>
                                                </p>
                                            </td>
                                            <td class="text-center">
                                                <span t-field="bo_line.product_uom_qty"/>
                                                <span t-field="bo_line.product_uom"/>
                                            </td>
                                        </tr>
                                    </t>
                                </tbody>
                            </table>
                        </t>

                        <div class="container row" style="margin-top:4em;margin-left:1em;float:right; font-size:15px;">
                            <div class="col-12">
                                <strong><span style="color:white;">Signature:</span> </strong><span style="color:white;">________________________</span>
                                <strong><span style="color:white;">Signature:</span> </strong><span style="color:white;">_________________________</span>
                                <strong>Signature: </strong><span style="color:black;">____________________________</span>
                            </div>
                        </div>

                        <div t-if="o.signature" class="mt32 ml64 mr4" name="signature">
                            <div class="offset-8">
                                <strong>Signature</strong>
                            </div>
                            <div class="offset-8">
                                <img t-att-src="image_data_uri(o.signature)" style="max-height: 4cm; max-width: 8cm;"/>
                            </div>
                            <div class="offset-8 text-center">
                                <p t-field="o.partner_id.name"/>
                            </div>
                        </div>

                    </div>
                </t>
            </t>
        </t>
    </template>
    <template id="report_deliveryslip_inh">
        <t t-foreach="docs" t-as="o">
            <t t-call="so_po_customization.report_delivery_document_inh" t-lang="o.partner_id.lang"/>
        </t>
    </template>

</odoo>