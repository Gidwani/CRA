                <!--                   </t>-->
                <!--                </t>-->
                <!--                <xpath expr="//div[@class='page']" position="replace">-->
                <t t-if="o.get_delivery() == 1">
                    <t t-if="o.state == 'done'">
                        <div class="page" style="margin-top:-55px;">
                            <h2>
                                <span t-field="o.name"/>
                            </h2><br/>
                            <div class="row mt32 mb32"  style="background-color:#E0E0E0;">
                                <div class="col-auto col-3 mw-100 mb-2">
                                    <strong>Customer Name:</strong>
                                    <p class="m-0" t-field="o.partner_id.name"/>
                                </div>

                                <div t-if="o.partner_id.street or o.partner_id.city or o.partner_id.country_id.name"  class="col-auto col-3 mw-100 mb-2">
                                    <strong>Company Address:</strong>
                                    <p t-if="o.partner_id.street" class="m-0" t-field="o.partner_id.street"/>
                                    <p t-if="o.partner_id.city" class="m-0" t-field="o.partner_id.city"/>
                                    <p t-if="o.partner_id.country_id.name" class="m-0" t-field="o.partner_id.country_id.name"/>
                                </div>
                                <div t-if="o.partner_id.vat" class="col-auto col-3 mw-100 mb-2">
                                    <strong> TRN:</strong>
                                    <p class="m-0" t-field="o.partner_id.vat"/>
                                </div>
                            </div>


                            <div class="row mt32 mb32">
                                <div t-if="o.origin" class="col-auto" name="div_origin">
                                    <strong>SO #:</strong>
                                    <p t-field="o.origin"/>
                                </div>
                                <div t-if="o.state" class="col-auto" name="div_sched_date">
                                    <strong>DO Date:</strong>
                                    <t t-if="o.state == 'done'">
                                        <p t-field="o.date_done" t-options='{"widget": "date"}'/>
                                    </t>
                                    <t t-if="o.state != 'done'">
                                        <p t-field="o.scheduled_date" t-options='{"widget": "date"}'/>
                                    </t>
                                </div>
                                <div t-if="o.sale_id.client_order_ref" class="col-auto" name="div_sched_date">
                                    <strong>PO #:</strong>
                                    <t t-if="o.state == 'done'">
                                        <p t-field="o.sale_id.client_order_ref"/>
                                    </t>
                                    <t t-if="o.state != 'done'">
                                        <p t-field="o.sale_id.client_order_ref"/>
                                    </t>
                                </div>
                                <div t-if="o.sale_id.incoterm" class="col-auto">
                                    <strong>Incoterm:</strong>
                                    <p class="m-0" t-field="o.sale_id.incoterm"/>
                                </div>
                            </div>
                            <table class="table table-sm" t-if="o.state!='done'" name="stock_move_table">
                                <thead>
                                    <tr>
                                        <th name="th_sm_sr" class="text-center"><strong>SR #</strong></th>
                                        <th name="th_sm_sr" class="text-center"><strong>Ref #</strong></th>
                                        <th name="th_sm_product" class="text-center"><strong>Products</strong></th>
                                        <th name="th_sm_quantity" class="text-center"><strong>QTY</strong></th>
                                        <th name="th_sm_quantity" class="text-center"><strong>UOM</strong></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <t t-set="lines" t-value="o.move_lines.filtered(lambda x: x.product_uom_qty)"/>
