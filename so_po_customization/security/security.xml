<odoo>
    <data>
        <!-- Security Groups -->
        <record id="group_provision_access" model="res.groups">
            <field name="name">Provision Access</field>
        </record>

        <record id="group_provision_sales" model="res.groups">
            <field name="name">Provision - Sales</field>
            <field name="implied_ids" eval="[(4, ref('group_provision_access'))]"/>
        </record>

        <record id="group_provision_purchase" model="res.groups">
            <field name="name">Provision - Purchase</field>
            <field name="implied_ids" eval="[(4, ref('group_provision_access'))]"/>
        </record>

        <record id="group_provision_invoice" model="res.groups">
            <field name="name">Provision - Invoice</field>
            <field name="implied_ids" eval="[(4, ref('group_provision_access'))]"/>
        </record>

        <record id="group_provision_bills" model="res.groups">
            <field name="name">Provision - Bills</field>
            <field name="implied_ids" eval="[(4, ref('group_provision_access'))]"/>
        </record>

        <record id="group_provision_return_invoice" model="res.groups">
            <field name="name">Provision - Return Invoice</field>
            <field name="implied_ids" eval="[(4, ref('group_provision_access'))]"/>
        </record>

        <record id="group_provision_return_bill" model="res.groups">
            <field name="name">Provision - Return Bill</field>
            <field name="implied_ids" eval="[(4, ref('group_provision_access'))]"/>
        </record>

        <record id="group_provision_grn" model="res.groups">
            <field name="name">Provision - GRN</field>
            <field name="implied_ids" eval="[(4, ref('group_provision_access'))]"/>
        </record>

        <record id="group_provision_delivery" model="res.groups">
            <field name="name">Provision - Delivery Orders</field>
            <field name="implied_ids" eval="[(4, ref('group_provision_access'))]"/>
        </record>
    </data>
</odoo>